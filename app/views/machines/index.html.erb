<p id="notice"><%= notice %></p>

<h1>Machines</h1>

<table>
  <thead>
    <tr>
      <th>Accumulated</th>
      <th>Inserted</th>
      <th>Id user</th>
      <th>Month</th>
      <th>Historical</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @machines.each do |machine| %>
      <tr>
        <td><%= machine.accumulated %></td>
        <td><%= machine.inserted %></td>
        <td><%= machine.id_user %></td>
        <td><%= machine.month %></td>
        <td><%= machine.historical %></td>
        <td><%= link_to 'Show', machine %></td>
        <td><%= link_to 'Edit', edit_machine_path(machine) %></td>
        <td><%= link_to 'Destroy', machine, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Machine', new_machine_path %>
